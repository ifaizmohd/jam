    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "rollup": "^4.40.1",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "cross-fetch": "^4.1.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3"
  },
  "_moduleAliases": {
    "@core": "./dist/core"
